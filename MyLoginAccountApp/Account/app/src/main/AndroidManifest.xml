<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.myaccount">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyAccount"
        tools:targetApi="31">
        <service
            android:name=".MyService"
            android:enabled="true"
            android:exported="true">
            <intent-filter >
                <action android:name="com.example.service.action"/>
            </intent-filter>
        </service>

        <activity
            android:name=".view.MainActivity"
            android:exported="true"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.AdminActivity"
            android:exported="true"
            android:launchMode="singleTop"></activity>
        <activity
            android:name=".view.AdminLoginActivity"
            android:exported="true"
            android:launchMode="singleTop"></activity>
        <activity
            android:name=".view.UserLoginActivity"
            android:exported="true"
            android:launchMode="singleTop"></activity>
        <activity
            android:name=".view.UserRegisterActivity"
            android:exported="true"
            android:launchMode="singleTop"></activity>
        <activity
            android:name=".view.UserChangeActivity"
            android:exported="true"
            android:launchMode="singleTop"></activity>

        <provider
            android:name=".bean.UserProvider"
            android:authorities="com.example.myaccount"
            android:enabled="true"
            android:exported="true" />
    </application>
    <queries>
        <package android:name="com.example.myaccount" />
    </queries>
</manifest>